<%= simple_form_for resource do |f| %>
  <%= f.input :name %>

  <%= f.input :visibility, as: :select, wrapper: :selectpicker, include_blank: false,
    collection: group_visibility_select_options %>

  <%= f.input :description %>

  <%= f.input :starts_at, as: :string %>

  <%= f.input :ends_at, as: :string %>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
